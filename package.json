{
  "name": "high-sentences",
  "version": "1.0.0",
  "description": "Nothing to say now, perhaps later",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "start": "node dist/main/index.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kafir-coder/high-sentences.git"
  },
  "keywords": [
    "sentences"
  ],
  "author": "Caio Tony",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kafir-coder/high-sentences/issues"
  },
  "homepage": "https://github.com/kafir-coder/high-sentences#readme",
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
